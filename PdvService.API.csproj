<?xml version="1.0"?>
<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <OutputType>Exe</OutputType>
    <Platforms>AnyCPU;x86;x64</Platforms>
    <PackageId>PdvService</PackageId>
    <Authors>Equipe ERP 2</Authors>
    <Company>Telecon Sistemas</Company>
    <Product>Torus PDV</Product>
    <Version>7.0.0</Version>
    <RootNamespace>PdvService.API</RootNamespace>
    <AssemblyName>PdvService</AssemblyName>
    <NeutralLanguage>pt-BR</NeutralLanguage>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
    <AnalysisMode>AllEnabledByDefault</AnalysisMode>
    <IncludeOpenAPIAnalyzers>true</IncludeOpenAPIAnalyzers>
    <UserSecretsId>3698032d-5d1b-4de9-94dc-577c6926748c</UserSecretsId>
